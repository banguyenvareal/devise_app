<% if user_signed_in? %>

  <p>email: <%= current_user[:email] %></p>
  <p>name: <%= current_user[:name] %></p>
  <p>uid facebook: <%= current_user[:uid_facebook] %></p>
  <p>uid google: <%= current_user[:uid_google] %></p>
  <% if current_user[:uid_facebook].nil? %>
    <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %> <br>
  <% end %>

  <% if  current_user[:uid_google].nil? %>
    <%= link_to "Sign in with google", user_google_oauth2_omniauth_authorize_path %> <br>
  <% end %>

  <%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% else %>
  <br>
  <%= link_to "Sign in with Google", user_google_oauth2_omniauth_authorize_path %> <br>
  <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %> <br>
  <%= link_to "Sign up", new_user_registration_path %>
<% end %>
